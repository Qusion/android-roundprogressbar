<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="max" format="integer" />
        <attr name="zeroProgressEnabled" format="boolean" />
        <attr name="progress" format="integer" />
        <attr name="progressColor" format="color" />
        <attr name="backgroundColor" format="color" />
        <attr name="endCapsSize" format="dimension" />
        <attr name="endCapsVisible" format="boolean" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="backgroundStrokeWidth" format="dimension" />
        <attr name="animationDuration" format="integer" />
        <attr format="integer" name="style" >
            <flag name="full" value="0"/>
            <flag name="half" value="1"/>
        </attr>
        <attr format="integer" name="backgroundStyle">
            <flag name="full" value="0"/>
            <flag name="dotted" value="1"/>
        </attr>

    </declare-styleable>
</resources>