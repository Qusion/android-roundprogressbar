<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="max" format="integer" />
        <attr name="zero_progress_enabled" format="boolean" />
        <attr name="progress" format="integer" />
        <attr name="progress_color" format="color" />
        <attr name="background_color" format="color" />
        <attr name="end_cap_size" format="dimension" />
        <attr name="end_cap_visible" format="boolean" />
        <attr name="stroke_width" format="dimension" />
        <attr name="background_stroke_width" format="dimension" />
        <attr name="animation_duration" format="integer" />
        <attr format="integer" name="style" >
            <flag name="full" value="0"/>
            <flag name="half" value="1"/>
        </attr>
        <attr format="integer" name="background_style">
            <flag name="full" value="0"/>
            <flag name="dotted" value="1"/>
        </attr>

    </declare-styleable>
</resources>